<picture>
    <source type="image/jpeg" media="(max-width: 480px)" srcset="{{ image.customsize(480, 320) }}">
    <source type="image/jpeg" media="(max-width: 480px)" srcset="{{ image.customsize(768, 512) }}">
    <source type="image/jpeg" media="(max-width: 1200px)" srcset="{{ image.customsize(1200, 600) }}">
    <source type="image/jpeg" media="(max-width: 1600px)" srcset="{{ image.customsize(1600, 800) }}">
    <source type="image/jpeg" media="(min-width: 1601px)" srcset="{{ image.customsize(1800, 900) }}">
    <img src="{{ image.customsize(1600, 800) }}" class="block w-full lazyload">
</picture>
